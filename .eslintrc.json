{
    "env": {
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "overrides": [
        {
            "files": [
                "**/*.{js,mjs,cjs,ts}"
            ]
        }
    ],
    "rules": {
        "@typescript-eslint/no-explicit-any": "off",
        "no-unused-vars": "off"
    }
}